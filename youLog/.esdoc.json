{
  "source": "./components",
  "destination": "./docs",
  "excludes": ["node_modules", ".expo-shared", ".expo", "doc"],
  "plugins": [
    {"name": "esdoc-standard-plugin"}, 
    {"name": "esdoc-react-plugin"},
    {"name": "esdoc-ecmascript-proposal-plugin", "option": {"all": true}},
    {"name": "esdoc-jsx-plugin"}
  ]
}
